<div class="pt-4">
  <div class="ml-2 d-flex align-self-center">
    <div class="mx-2">
      <i class="fas fa-chevron-right fa-2x"></i>
    </div>
    <div class="mx-2 d-flex">
      <h4><strong>Annuler des cr√©neaux</strong></h4>
    </div>
  </div>
  <br>

  <ul class="list-group p-3" >
    <div class="rounded schedule-items" id="schedule-container">
    <% @schedules.each do |schedule| %>
      <% if schedule.is_booked? %>
        <%= render "schedule_booked", schedule: schedule %>
      <% else %>
        <%= render "schedule_object", schedule: schedule %>
      <% end %>
    <% end %>
    </div>
  </ul>
</div>
