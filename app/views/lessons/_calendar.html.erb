<div class="card mb-3 bg-white text-dark">
  <h4 class="card-header bg-white text-center mt-3">Voici les disponibilit√©s du prof</h4>
  <div class="bg-dark text-white p-3 rounded-bottom">
    <%= week_calendar(events: @schedules) do |date, schedules| %>
      <% schedules.each do |schedule| %>
        <% if schedule.is_booked? %>
          <%= render "badge_hour_booked", schedule: schedule %>
        <% else %>
          <%= render "badge_hour", schedule: schedule %>
        <% end %>
      <% end %>
    <% end %>
  </div>
</div>
